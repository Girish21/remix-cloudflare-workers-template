{
  "name": "remix-app",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build.d.ts",
  "scripts": {
    "build:css": "tailwindcss -m -i styles/app.css -o app/styles/app.css",
    "build:remix": "remix build",
    "build": "run-s build:*",
    "dev": "concurrently \"npm:dev:css\" \"npm:dev:remix\"",
    "dev:css": "tailwindcss -w -i styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "lint": "eslint .",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.10.0",
    "@remix-run/dev": "^1.8.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "cloudflare-env": "*",
    "concurrently": "^7.2.1",
    "eslint": "^8.23.0",
    "eslint-config-custom": "*",
    "isbot": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "tsconfig": "*",
    "typescript": "^4.9.4"
  }
}
